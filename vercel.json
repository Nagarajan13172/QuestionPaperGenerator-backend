{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "APP_NAME": "Question Paper Generator API",
    "DEBUG": "False",
    "HOST": "0.0.0.0",
    "PORT": "8000"
  }
}
